{
  "name": "strapi-plugin-favorite-views",
  "version": "1.0.0",
  "description": "A Strapi plugin for creating 'views' to access specific searches in a defined content type",
  "strapi": {
    "name": "favorite-views",
    "description": "Manage favorite views in Strapi's admin console",
    "kind": "plugin",
    "displayName": "Favorite Views"
  },
  "scripts": {
    "lint": "eslint",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --ignore-path .eslintignore",
    "format:check": "prettier --ignore-path .eslintignore --check .",
    "format:write": "prettier --ignore-path .eslintignore --write .",
    "prepare": "[ -x \"$(command -v husky)\" ] && husky install || echo \"husky command not found\"",
    "test": "jest"
  },
  "dependencies": {
    "@strapi/design-system": "^1.6.3",
    "@strapi/helper-plugin": "^4.6.0",
    "@strapi/icons": "^1.6.3",
    "prop-types": "^15.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "styled-components": "^5.3.6"
  },
  "peerDependencies": {
    "@strapi/strapi": "^4.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-router-dom": "^5.3.4",
    "styled-components": "^5.3.6"
  },
  "author": {
    "name": "Kaliop",
    "email": "support@kaliop.com",
    "url": "https://www.kaliop.com/"
  },
  "maintainers": [
    {
      "name": "Kaliop",
      "email": "support@kaliop.com",
      "url": "https://www.kaliop.com/"
    }
  ],
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
